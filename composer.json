{
    "name": "myolbd/unique-slug",
    "description": "A Laravel package to generate unique and SEO-friendly slugs for modelsy",
    "keywords": ["laravel","slug","unique-slug","slugify","eloquent","seo","url-slug","model-slug","slug-generator"],
    "homepage": "https://github.com/sujonmia019/unique-slug-laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Myol\\UniqueSlug\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sujon Mia",
            "email": "sujonbdjoin019@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Myol\\UniqueSlug\\SlugServiceProvider"
            ],
            "aliases": {
                "Slug": "Myol\\UniqueSlug\\Facades\\Slug"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
